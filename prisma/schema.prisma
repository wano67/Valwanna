generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Gift {
  id          String   @id @default(uuid())
  title       String
  url         String?
  description String?
  price       Float?
  currency    String?
  images      String?
  mainImage   String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
